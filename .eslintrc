{
  "env": {
  "browser": true,
  "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
  "Atomics": "readonly",
  "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
  "ecmaVersion": 2018,
  "sourceType": "module"
  },
  "rules": {
  "no-console": "off",
  "indent": ["error", "tab"],
  "no-tabs": 0,
  "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
  }